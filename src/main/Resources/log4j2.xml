<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <File name="LogFile" fileName="epuap.log">
            <PatternLayout>
                <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
            </PatternLayout>
        </File>

        <Async name="Async">
            <AppenderRef ref="LogFile"/>
        </Async>
    </Appenders>


    <Loggers>
        <Root level="info">
            <!--      <AppenderRef ref="Console" level="ALL" />-->
            <AppenderRef ref="Async" level="INFO"/>
        </Root>
    </Loggers>
</Configuration>